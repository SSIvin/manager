    <!--1.7.1.1 Справочник видов документов-оснований юридического события (действия)-->
    <xsl:template name="RL_codeType">
        <xsl:param name="kod"/>
        <xsl:choose>
            <xsl:when test="$kod = '0001'">Акт об отобрании ребенка</xsl:when>
            <xsl:when test="$kod = '0002'">Акт органа опеки и попечительства о временном назначении опекуна или попечителя (акт о предварительных опеке или попечительстве)</xsl:when>
            <xsl:when test="$kod = '0003'">Акт органа опеки и попечительства о назначении опекуна или попечителя</xsl:when>
            <xsl:when test="$kod = '0004'">Акт органа опеки и попечительства об объявлении дееспособным</xsl:when>
            <xsl:when test="$kod = '0005'">Акт органа опеки и попечительства об освобождении опекуна или попечителя от исполнения возложенных на них обязанностей либо об их отстранении от исполнения возложенных на них обязанностей</xsl:when>
            <xsl:when test="$kod = '0006'">Выписка из судебного решения</xsl:when>
            <xsl:when test="$kod = '0007'">Доверенность, иной документ</xsl:when>
            <xsl:when test="$kod = '0008'">Свидетельство о браке</xsl:when>
            <xsl:when test="$kod = '0009'">Свидетельство о рождении</xsl:when>
            <xsl:when test="$kod = '0011'">Свидетельство о смерти</xsl:when>
            <xsl:when test="$kod = '0012'">Свидетельство об установлении отцовства</xsl:when>
            <xsl:when test="$kod = '0013'">Иной документ</xsl:when>
            <xsl:when test="$kod = '0014'">Акт гражданского состояния ЕГР ЗАГС</xsl:when>
            <xsl:when test="$kod = ''"> </xsl:when>
        </xsl:choose>
    </xsl:template>
    <!--Код вида юридического события (действия)-->
    <xsl:template name="RL_codeEventType">
        <xsl:param name="kod"/>
        <xsl:choose>
            <xsl:when test="$kod = '001'">Лишение родительских прав</xsl:when>
            <xsl:when test="$kod = '002'">Восстановление в родительских правах</xsl:when>
            <xsl:when test="$kod = '003'">Ограничение родительских прав</xsl:when>
            <xsl:when test="$kod = '004'">Отмена ограничения родительских прав</xsl:when>
            <xsl:when test="$kod = '005'">Отобрание ребенка при непосредственной угрозе его жизни и здоровью</xsl:when>
            <xsl:when test="$kod = '006'">Возврат ребенка в семью после отобрания по решению суда</xsl:when>
            <xsl:when test="$kod = '007'">Признание гражданина недееспособным</xsl:when>
            <xsl:when test="$kod = '008'">Отмена признания гражданина недееспособным</xsl:when>
            <xsl:when test="$kod = '009'">Ограничение дееспособности гражданина</xsl:when>
            <xsl:when test="$kod = '010'">Отмена ограничения дееспособности гражданина</xsl:when>
            <xsl:when test="$kod = '011'">Объявление полностью дееспособным (эмансипация)</xsl:when>
            <xsl:when test="$kod = '012'">Отмена эмансипации</xsl:when>
            <xsl:when test="$kod = '013'">Приобретение дееспособности при вступлении в брак</xsl:when>
            <xsl:when test="$kod = '014'">Утрата дееспособности несовершеннолетним после объявления брака недействительным</xsl:when>
            <xsl:when test="$kod = '015'">Возникновение родительских прав</xsl:when>
            <xsl:when test="$kod = '016'">Отмена возникновения родительских прав в связи с отменой регистрации рождения по решению суда</xsl:when>
            <xsl:when test="$kod = '019'">Назначение опекуном недееспособного лица</xsl:when>
