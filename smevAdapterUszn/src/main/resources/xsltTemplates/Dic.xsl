    <xsl:template name="FnsKodCategNalog">
        <xsl:param name="kodCateg"/>
        <xsl:choose>
            <xsl:when test="$kodCateg = '720'">физическое лицо, зарегистрированное в качестве индивидуального предпринимателя</xsl:when>
            <xsl:when test="$kodCateg = '730'">нотариус, занимающийся частной практикой, и другие лица, занимающиеся в установленном действующим законодательством порядке частной практикой</xsl:when>
            <xsl:when test="$kodCateg = '740'">адвокат, учредивший адвокатский кабинет</xsl:when>
